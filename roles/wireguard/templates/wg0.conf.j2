[Interface]
Address = {{ wireguard_server_address }}
ListenPort = {{ wireguard_server_port }}
PrivateKey = {{ wireguard_server_private_key }}

{% for peer_name, peer_value in wireguard_peers.items() %}
[Peer]
# {{ peer_name }}
PublicKey =  {{ peer_value.public_key }}
PresharedKey = {{ peer_value.preshared_key }}
AllowedIPs = {{ peer_value.ip_address }}

{% endfor %}
