{% for key, value in grp_podman_networking.items() %}
server {
  listen 80;
  listen [::]:80;

  server_name {{ value.fulldomain }};

  location / {
      proxy_pass http://localhost:{{ value.webui_port }}/;
      proxy_set_header Host $host;
      proxy_http_version 1.1;
      proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
  }
}

{% endfor %}